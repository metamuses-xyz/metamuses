  "db_name": "PostgreSQL",
  "query": "\n            SELECT\n                tc.id,\n                tc.user_address,\n                tc.task_type,\n                tc.points_awarded,\n                tc.multiplier::DOUBLE PRECISION as \"multiplier!\",\n                tc.metadata,\n                tc.completed_at\n            FROM task_completions tc\n            WHERE tc.user_address = $1\n            ORDER BY tc.completed_at DESC\n            LIMIT $2\n            ",
